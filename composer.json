{
    "name": "pribolshoy/repository",
    "description": "Public library",
    "keywords": ["repository"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nikolay Pribolshoy",
            "email": "pribolshoy@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "phpmd/phpmd": "dev-master",
        "vimeo/psalm": "*",
        "rector/rector": "*",
        "phpstan/phpstan": "*",
        "phan/phan": "*",
        "vkcom/noverify": "dev-master",
        "qossmic/deptrac-shim": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "pribolshoy\\repository\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pribolshoy\\repository\\tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "cweagans/composer-patches": true
        }
    }
}
